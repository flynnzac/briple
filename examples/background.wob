task /t1
     (
       accept /instruction .
       move /ans /instr .
       accept /context .
       import ans .
       instr .
       queue /result ans .
     )
     [ registry ] .

run-task t1 .
queue /instruction ( add x y ) t1 .
queue /context [ registry /x 2 /y 4 ] t1 .
accept-or /result ( print "Result not ready yet." ) t1 .




     
