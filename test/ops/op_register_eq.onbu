#!./onbu -c
source "src/library/assert.onbu" .
error-messages False .

print "Start <register-eq> test." .

' Tests with correct usage

assert /test ( register-eq /hello /hello . ) .
assert /test ( not [ register-eq /hello /hell . ] . ) .
assert /test ( not [ register-eq /hello /hellop . ] . ) .

' Test with wrong argument type

assert /test ( is-error ( register-eq "hello" "hello" . ) . ) .

' Test with incorrect number of arguments

assert /test ( is-error ( register-eq /hello . ) . ) .

print "Finished <register-eq> test." .

