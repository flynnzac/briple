#!./onbu -c
source "src/library/assert.onbu" .
error-messages False .

print "Start <to-register> test." .

' Tests with correct usage

assert /test ( to-register 5 . register-eq ans /t5 . ) .
assert /test ( to-register "hello" . register-eq ans /hello . ) .

' Test with wrong argument type
assert /test ( is-error ( to-register 1.2 . ) . ) .

' Test with incorrect number of arguments

assert /test ( is-error ( to-register . ) . ) .

print "Finished <to-register> test." .

